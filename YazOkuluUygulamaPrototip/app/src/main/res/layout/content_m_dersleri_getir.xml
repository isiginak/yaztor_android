<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffff0"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.android.yazokuluuygulamaprototip.mDersleriGetir"
    tools:showIn="@layout/app_bar_m_dersleri_getir">
    <TextView
        android:id="@+id/ana_baslik"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hadi Okulunu Bul!!!  "
        android:textAlignment="center"
        android:padding="10dp"
        android:textColor="#000"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="-3dp" />

    <ListView
        android:id="@+id/liste"
        android:descendantFocusability="blocksDescendants"
        android:layout_below="@+id/ana_baslik"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" />
</RelativeLayout>
