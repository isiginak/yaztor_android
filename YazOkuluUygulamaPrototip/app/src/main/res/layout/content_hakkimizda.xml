<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffff0"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.android.yazokuluuygulamaprototip.Hakkimizda"
    tools:showIn="@layout/app_bar_hakkimizda">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="  Geliştiriciler:
"
        android:textSize="24sp"
        android:textColor="#000"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="176dp" />
    <TextView
        android:id="@+id/text"
        android:layout_below="@id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="  Bahar Uzun"
        android:textSize="24sp"
        android:textColor="#000"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/text2"
        android:layout_below="@id/text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="  Fatma Boztaş "
        android:textSize="24sp"
        android:textColor="#000"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/text3"
        android:layout_below="@id/text2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="  İhbal Sığınak"
        android:textSize="24sp"
        android:textColor="#000"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
